<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_814pxh"
    tools:context=".modules.home.ui.ui.home.HomeFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerListWorker"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_13pxh"
        android:layout_marginStart="@dimen/_9pxh"
        android:layout_marginTop="@dimen/_12pxh"
        android:layout_marginBottom="@dimen/_12pxh"
        android:layout_width="match_parent"
        android:nestedScrollingEnabled="false"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        app:layoutManager="androidx.recyclerview.widget.StaggeredGridLayoutManager"
        tools:listitem="@layout/item_worker"
        tools:itemCount="2"/>

</FrameLayout>